            "path": "https://developer.salesforce.com/docs/atlas.en-us.sfdx_cli_reference.meta/sfdx_cli_reference/cli_reference_unified.htm",
            "description": "",
            "type": "link",
            "groupId": "salesforce"
        }
    ],
    "squarl.groups": [
        {
            "id": "mcp",
            "name": "Model Context Protocol"
        },
        {
            "id": "salesforce",
            "name": "Salesforce"
        }
    ],
    "workbench.editor.highlightModifiedTabs": true,
    "chat.mcp.discovery.enabled": false,
    "search.exclude": {
        "**/dist/*": true,
        "**/node_modules/**": true
    },
    "outline.showFunctions": true,
    "outline.showFiles": true,
    "outline.showFields": false,
    "outline.showConstants": false,
    "outline.showVariables": true,
    "outline.showMethods": true,
    "outline.showModules": true,
    "eslint.useFlatConfig": true,
    "eslint.workingDirectories": [
        {
            "mode": "auto"
        }
    ],
    "eslint.validate": [
        "javascript",
        "javascriptreact"
    ],
    "eslint.options": {
        "overrideConfigFile": "eslint.config.js"
    },
    "actionButtons": {
        "commands": [
            {
                "name": "$(test-view-icon) Inspect",
                "useVsCodeApi": true,
                "command": "workbench.action.tasks.runTask",
                "args": ["npm: startMcpInspector"],
                "color": "#a0a0d0"
            },
            {
                "name": "$(edit) Lint",
                "useVsCodeApi": true,
                "command": "workbench.action.tasks.runTask",
                "args": ["npm: lint:fix"],
                "color": "#4354a8"
            },
            {
                "name": "$(testing-debug-icon) Test",
                "useVsCodeApi": true,
                "command": "workbench.action.tasks.test",
                "color": "#bb9955"
            },
            {
                "name": "$(send) Publish",
                "useVsCodeApi": true,
                "command": "workbench.action.tasks.runTask",
                "args": ["npm: publish-package"],
                "color": "#cc8888"
            }
        ],
        "defaultColor": "#ffffe0",
        "reloadButton": null,
        "loadNpmCommands": false
    },
    "explorer.fileNesting.patterns": {
        "*.js": "${capture}.md"
    },
    "notebook.outline.showCodeCells": false,
    "editor.codeActionsOnSave": {
        // "source.fixAll.eslint": "explicit",
        // "source.organizeImports": "explicit"
    },
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#e788ff7c",
        "editor.selectionHighlightBackground": "#ff00005b",
        "tab.activeForeground": "#ffffff",
        "tab.activeBorder": "#006bc9",
        "tab.activeModifiedBorder": "#f4a238",
        "statusBar.background": null
    },
    "typescript.workspaceSymbols.excludeLibrarySymbols": false,
    "todohighlight.keywords": [
        "TODO",
    ]
}