{
	"name": "test_research4",
	"version": "0.3.27",
	"description": "Test Research 4",
	"main": "index.js",
	"type": "module",
	"browser": {
		"child_process": false
	},
	"bin": {
		"test_research4": "bin/cli.js"
	},
	"scripts": {
		"publish-package": "sh dev/publish-package.sh",
		"publish-package-skip-tests": "sh dev/publish-package.sh --skip-tests",
		"test salesforceMcpUtils getState": "node test/runner.js --tests=\"salesforceMcpUtils getState\"",
		"test salesforceMcpUtils": "node test/runner.js --tests=\"salesforceMcpUtils\"",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"coverage": "vitest run --coverage",
		"lint:fix": "biome check . --write --reporter=summary",
		"startMcpInspector": "sh dev/startMcpInspector.sh"
	},
	"keywords": [
		"test",
		"research"
	],
	"author": "Trevor Smart",
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.17.4",
		"conf": "^14.0.0",
		"env-paths": "^3.0.0",
		"fs-extra": "^11.3.1",
		"node-fetch": "^3.3.2",
		"node-schedule": "^2.1.1",
		"os": "^0.1.2",
		"playwright": "^1.55.0",
		"semver": "^7.5.4",
		"zod": "^3.24.2"
	},
	"engines": {
		"node": ">=22.7.0 <23"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@modelcontextprotocol/inspector": "^0.16.5",
		"@vitest/coverage-v8": "^3.2.4",
		"globals": "^16.3.0",
		"ibm-test-mcp-client": "^0.0.52",
		"javascript-obfuscator": "^4.1.0",
		"vitest": "^3.2.4"
	}
}
